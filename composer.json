{
    "name": "mix-plus/validation",
    "description": "mix-plus validation",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "MixPlus\\Validation\\": "src/"
        }
    },
    "authors": [
        {
            "name": "cexll",
            "email": "evanxian9@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "phpunit/phpunit": "^9.5",
        "jchook/phpunit-assert-throws": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "MixPlus\\Test\\Validation\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always"
    }
}
